<Window x:Class="SimpleVideoEncoder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleVideoEncoder"
        mc:Ignorable="d"
        Title="MainWindow" Height="242" Width="521" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,0,32">
        <TextBlock x:Name="TxbEta" HorizontalAlignment="Left" Margin="417,108,0,0" TextWrapping="Wrap" Text="X%" VerticalAlignment="Top" Width="30" Height="24" RenderTransformOrigin="11.054,0.208"/>
        <Button x:Name="BtnNewEncoding" Content="New" HorizontalAlignment="Left" Height="23" Margin="417,59,0,0" VerticalAlignment="Top" Width="61" RenderTransformOrigin="1.033,0.739" Click="BtnNewEncoding_Click"/>
        <ProgressBar x:Name="PgbCompletion" Height="24" Margin="43,108,117,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TxtVideo" HorizontalAlignment="Left" Height="23" Margin="43,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="342" RenderTransformOrigin="-0.817,-6.652" IsReadOnly="True" MaxLines="1"/>

    </Grid>
</Window>
